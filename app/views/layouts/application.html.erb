<!DOCTYPE html>
<html>
<head>
  <title>LendApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">


<% if current_user %>
  Signed in as <%= current_user.email %> | <%= link_to "Logout", sessions_path, method: :delete %>
<% else %>
  <%= link_to 'Log In', '/login' %> | <%= link_to 'Signup', '/signup' %>
<% end %>



<%= yield %>



</div>
</body>
</html>
