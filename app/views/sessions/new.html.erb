<div class="newitem center-align">

<h1>Log In</h1>
<div class="container">
<%= form_tag sessions_path, method: "post" do %>
	<span>Email: </span><br>
	<%= text_field_tag :email, nil, required: true, placeholder: "Email" %><br>
	<span>Password: </span><br>
	<%= password_field_tag :password, nil, required: true, placeholder: "Password" %><br>
	<%= submit_tag "Log In" %>
<% end %>
</div>
</div>